{
  "name": "buildit",
  "version": "0.0.1",
  "description": "frontend build environment",
  "repository": {
    "type": "git",
    "url": "git://github.com/rootdigga/buildit.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^1.8.2",
    "browserify": "^8.0.2",
    "browserify-shim": "^3.8.2",
    "coffeeify": "^1.0.0",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-changed": "^1.1.0",
    "gulp-imagemin": "^2.1.0",
    "gulp-jade": "^0.10.0",
    "gulp-notify": "^2.1.0",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-sass": "^1.2.4",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-util": "^3.0.1",
    "hbsfy": "^2.2.1",
    "jquery": "~2.1.0",
    "pretty-hrtime": "^0.2.2",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.2.1"
  }
}
